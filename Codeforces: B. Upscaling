link: https://codeforces.com/problemset/problem/1950/B
#include <bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin>>t;
    while(t--){
      int n;
      cin>>n;
     for( int i=0 ; i<n ; i++ ){
        if( i%2 == 0 ){
            for( int j=0 ; j<n ; j++ ){
                if( j%2 == 0 ) cout << "##" ;
                else cout << ".." ;
            }
            cout << endl ;
            for( int j=0 ; j<n ; j++ ){
                if( j%2 == 0 ) cout << "##" ;
                else cout << ".." ;
            }
        }else{
            for( int j=0 ; j<n ; j++ ){
                if( j%2 == 1 ) cout << "##" ;
                else cout << ".." ;
            }
            cout << endl ;
            for( int j=0 ; j<n ; j++ ){
                if( j%2 == 1 ) cout << "##" ;
                else cout << ".." ;
            }
        }
        cout << endl; 
    }
            
    }
}
